<body>
    <div class="site_wrapper">
        <div id="header">
            <div id="trueHeader">
                <div class="wrapper">
                    <div class="container">
                        <!-- Menu -->
                        <div class="mainMenu">
                            <label for="toggleMenu" class="menuTitle">MENU</label>
                            <input type="checkbox" id="toggleMenu" />
                            <ul class="menu">

                                @foreach (var modulo in ViewBag.ListaModulos)
                                {
                                    <li><a href="~/Inicio/#section-@modulo.IdModulo">@modulo.NombreModulo</a></li>
                                }
                                <li><a href="#" class="btn-primary btn-menu"><i class="fa fa-desktop"></i> Solicitud en Línea</a></li>
                                <li><a href="#" class="btn-success btn-menu"><i class="fa fa-phone"></i> PBX: 742 58 77 </a></li>
                            </ul>
                        </div>
                        <!-- end menu -->
                    </div>
                </div>
            </div>
        </div>
        <div class="clearfix"></div>

        <div class="section_holder12">
            <div class="container">
                <a href="../Inicio/#section-2" class="pull-left"><i class="fa fa-reply-all"></i> Volver</a>
                <h1 class="lt_title_big"><span>Misión</span></h1>
                <div class="lt_title_line"></div>
                <div class="clearfix"></div>

                <div class="container" style="text-align:justify !important; width:80% !important; margin:0 auto !important">
                    @ViewBag.Mision
                </div>
            </div>
        </div>
    </div>
</body>
</html>
