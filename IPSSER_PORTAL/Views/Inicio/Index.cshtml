    <div class="site_wrapper">
        <div id="header">
            <div id="trueHeader">
                <div class="wrapper">
                    <div class="container">
                        <!-- Menu -->
                        <div class="mainMenu">
                            <label for="toggleMenu" class="menuTitle">MENU</label>
                            <input type="checkbox" id="toggleMenu" />
                            <ul class="menu">

                                @foreach (var modulo in ViewBag.ListaModulos)
                                {
                                    if (modulo.IdModulo == 7)
                                    {
                                        <li><a href="~/../Galeria/Index" onclick="$('#section-@modulo.IdModulo').animatescroll();">@modulo.NombreModulo</a></li>
                                    }
                                    else if (modulo.IdModulo == 1)
                                    {
                                        <li><a href="#" class="current" onclick="$('#section-@modulo.IdModulo').animatescroll();">@modulo.NombreModulo</a></li>
                                    }
                                    else
                                    {
                                        <li><a href="#" onclick="$('#section-@modulo.IdModulo').animatescroll();">@modulo.NombreModulo</a></li>
                                    }

                                }
                                <li><a href="#" class="btn-primary btn-menu"><i class="fa fa-desktop"></i> Solicitud en Línea</a></li>
                                <li><a href="#" class="btn-success btn-menu"><i class="fa fa-phone"></i> PBX: 742 58 77 </a></li>
                            </ul>
                        </div>
                        <!-- end menu -->
                    </div>
                </div>
            </div>
        </div>
        <div class="clearfix"></div>

        <!-- Sección Inicio -->
        <div id="section-1">
            <div class="tp-banner-container">
                <div class="tp-banner2">
                    <ul>
                        @foreach (var FrontSlide in ViewBag.ListaInicio)
                        {
                            <!-- SLIDE  -->
                            <li data-transition="fade" data-slotamount="1" data-masterspeed="1500" data-thumb="images/sliders/slider1_thumb.jpg" data-delay="13000" data-saveperformance="off" data-title="Our Workplace">

                                <!-- MAIN IMAGE -->

                                <img src="@FrontSlide.Fondo" alt="@FrontSlide.Fondo" data-bgposition="left center" data-kenburns="on" data-duration="14000" data-ease="Linear.easeNone" data-bgfit="100" data-bgfitend="130" data-bgpositionend="right center">
                                <!-- LAYERS -->

                                <div class="tp-caption offerbadge sfb"
                                     data-x="928"
                                     data-hoffset="0"
                                     data-voffset="-10"
                                     data-y="200"
                                     data-speed="800"
                                     data-start="1600"
                                     data-easing="Power4.easeOut"
                                     data-endspeed="300"
                                     data-endeasing="Power1.easeIn"
                                     data-captionhidden="off"
                                     style="z-index: 6">
                                    <img src="../images/logo.png" class="logo-slider">
                                </div>
                                <div class="tp-caption text15 white sfb"
                                     data-x="0"
                                     data-hoffset="0"
                                     data-voffset="-10"
                                     data-y="168"
                                     data-speed="800"
                                     data-start="2200"
                                     data-easing="Power4.easeOut"
                                     data-endspeed="300"
                                     data-endeasing="Power1.easeIn"
                                     data-captionhidden="off"
                                     style="z-index: 6">@FrontSlide.Titulo</div>
                                    @{int start = 2500, y = 220;}
                                    @for (var i = 1; i <= 3; i++)
                                    {
                                        <div class="tp-caption slide_list3 sfb"
                                             data-x="0"
                                             data-hoffset="0"
                                             data-voffset="-10"
                                             data-y="@y"
                                             data-speed="800"
                                             data-start="@start"
                                             data-easing="Power4.easeOut"
                                             data-endspeed="300"
                                             data-endeasing="Power1.easeIn"
                                             data-captionhidden="off"
                                             style="z-index: 6">
                                            @if (i == 1 && @FrontSlide.Contenido1 != null)
                                            {
                                                <i class="fa fa-check"></i>
                                                @FrontSlide.Contenido1;
                                            }
                                            @if (i == 2 && @FrontSlide.Contenido2 != null)
                                            {
                                                <i class="fa fa-check"></i>
                                                @FrontSlide.Contenido2;
                                            }
                                            @if (i == 3 && @FrontSlide.Contenido3 != null)
                                            {
                                                <i class="fa fa-check"></i>
                                                @FrontSlide.Contenido3;
                                            }
                                            @{start = start + 300; }
                                            @{ y = y + 30;}
                                        </div>
                                    }
                                </li>
                        }
                    </ul>
                    <div class="tp-bannertimer"></div>

                </div>
            </div>
        </div>
        <div class="clearfix"></div>
